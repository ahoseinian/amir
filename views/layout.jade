doctype html
html(ng-app="myApp")
  head
    title= title
    link(rel='stylesheet', href='/stylesheets/style.css')
    link(rel='stylesheet', href='/bootstrap/dist/css/bootstrap.min.css')
  body
    .container
      nav.navbar.navbar-light.bg-faded.m-b
        a.navbar-brand(href='#') Navbar
          ul.nav.navbar-nav
            li.nav-item.active
              a.nav-link(ui-sref='home') Home 
            li.nav-item
              a.nav-link(ui-sref='products') Products
            li.nav-item
              a.nav-link(ui-sref='users') Users
            li.nav-item
              a.nav-link(ui-sref='models.new') Models
          form.form-inline.navbar-form.pull-right
            input.form-control(type='text', placeholder='Search')
            button.btn.btn-success-outline(type='submit') Search

      block content

    script(src='/jquery/dist/jquery.min.js')
    script(src='/bootstrap/dist/js/bootstrap.min.js')
